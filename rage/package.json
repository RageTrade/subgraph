{
  "name": "perpswap-contracts-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create rage/perpswap-contracts-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create rage/perpswap-contracts-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "auth": "graph auth https://thegraph.com/hosted-service/subgraph/fr0ntenddev/rage-trade <add-key-here>",
    "deploy": "graph deploy fr0ntenddev/rage-trade --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "build": "graph build",
    "deploy-local": "graph deploy rage/perpswap-contracts-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "format": "prettier --check '**/*'",
    "format:fix": "prettier --write '**/*'",
    "update": "pnpm add @ragetrade/sdk@latest && node update-subgraph.js"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "fs-extra": "^10.0.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "@ragetrade/sdk": "^0.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
